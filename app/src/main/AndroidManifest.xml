        <activity android:name=".twinklingrefreshlayout.WebActivity"/>
        <activity android:name=".twinklingrefreshlayout.NestedLayoutActivity"/>
        <activity android:name=".twinklingrefreshlayout.CoordinateActivity"/>
        <activity android:name=".twinklingrefreshlayout.NormalViewActivity"/>
        <activity android:name=".twinklingrefreshlayout.TestActivity"/>

    </application>

</manifest>